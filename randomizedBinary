public static int randomizedBinary(int[] arr, int target, int left, int right){
        if (left > right) return -1; // Base case for when the range is invalid
        Random random = new Random();
        int partition = random.nextInt(right - left + 1) + left; // Random index between left and right
        if (arr[partition] == target) return partition; // Target found
        else if (arr[partition] > target)return randomizedBinary(arr, target, left, partition - 1); // Search left subarray
        else return randomizedBinary(arr, target, partition + 1, right); // Search right subarray
    }
